{
  "name": "auth-microservices-and-redis",
  "version": "1.0.0",
  "description": "node project with auth, microservices and redis",
  "main": "index.js",
  "scripts": {
    "start": "pm2 start ecosystem.config.js --watch --env production",
    "dev": "pm2 start ecosystem.config.js --watch --env development",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "migrations:seed": "sequelize-cli db:seed:all"
  },
  "author": "Gustavo Mart√≠nez <gustavomartinez1990@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "axios": "^1.1.3",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.6.4",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "redis": "^4.3.1",
    "sequelize": "^6.25.3"
  },
  "devDependencies": {
    "sequelize-cli": "^6.5.2"
  }
}
